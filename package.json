      "OPCUA-IIoT-Result-Filter": "opcuaIIoT/opcua-iiot-result-filter.js",
      "OPCUA-IIoT-Method-Caller": "opcuaIIoT/opcua-iiot-method-caller.js",
      "OPCUA-IIoT-Discovery": "opcuaIIoT/opcua-iiot-discovery.js"
    }
  },
  "engines": {
    "node": ">=6.10"
  },
  "scripts": {
    "test": "jest",
    "test-with-coverage": "istanbul cover _mocha --report lcovonly -- --recursive --timeout 10000 -R spec && cat ./coverage/lcov.info | codacy-coverage --token eded179f99764906b775603a52e8a5b4 && rm -rf ./coverage",
    "coverage": "standard --fix && gulp build && npm test && istanbul cover _mocha -- --recursive --timeout 10000",
    "build": "standard --fix && gulp publish",
    "prepublishOnly": "standard --fix && npm run build && npm test && mocha test --recursive --timeout 10000 --reporter dot",
    "ci-publish": "ci-publish",
    "release": "standard-version -a",
    "rewrite-changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "files": [
    "docs",
    "examples",
    "opcuaIIoT"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "ci-publish": "^1.3.1",
    "codacy-coverage": "^3.0.0",
    "conventional-changelog-cli": "^2.0.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-clean": "^0.4.0",
    "gulp-htmlmin": "^4.0.0",
    "gulp-jsdoc3": "^2.0.0",
    "gulp-obfuscate": "^0.2.9",
    "gulp-replace": "^1.0.0",
    "gulp-sequence": "^1.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^5.0.0-alpha.3",
    "gulp-uglify": "^3.0.1",
    "istanbul": "0.4.5",
    "jest": "^23.5.0",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nock": "^9.4.3",
    "node-red": "^0.19.0",
    "node-red-node-test-helper": "^0.1.8",
    "pump": "^3.0.0",
    "should": "^13.2.2",
    "sinon": "^6.1.4",
    "standard": "^11.0.1",
    "standard-version": "^4.4.0",
    "supertest": "^3.1.0",
    "uglify-js": "^3.4.6",
    "uglify-js-harmony": "^2.7.7",
    "when": "^3.7.8"
  },
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "test"
  },
  "standard": {
    "env": [ "jest" ],
    "ignore": [
      "code/",
      "node_modules/",
      "examples/",
      "opcuaIIoT/",
      "docs",
      "src/public/",
      "test"
    ]
  },
  "main": "index.js"
}
