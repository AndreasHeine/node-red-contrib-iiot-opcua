  "description": "The next evolution IoT/IIoT OPC UA toolbox package for Node-RED based on node-opcua.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/biancode/node-red-contrib-iiot-opcua.git"
  },
  "dependencies": {
    "better-assert": "1.0.2",
    "collections": "5.1.5",
    "cron": "1.3.0",
    "debug": "3.1.0",
